page {
  background: #fff;
  height: calc(100% - 88rpx);
}
.login-type {
  padding-top: 80rpx;
}
.login-type .logo {
    text-align: center;
    margin-bottom: 80rpx;
}
.login-type .logo image {
      width: 180rpx;
      height: 180rpx;
      border-radius: 28rpx;
}
.login-type .confirm-btn {
    width: 84%;
    margin: 0 7% 50rpx;
    height: 84rpx;
    line-height: 84rpx;
    border-radius: 12rpx;
    background-color: #47885e;
    color: #fff;
    font-size: 32rpx;
}
.login-type .plain {
    background: none;
    color: #47885e;
    border: 1rpx solid #47885e;
}
.login-type .footer {
    width: 100%;
    text-align: center;
    position: fixed;
    bottom: 40rpx;
    font-size: 26rpx;
}
.login-type .footer .protocol {
      color: #47885e;
      margin: 0 10rpx;
}

